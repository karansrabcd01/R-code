A<-diag(c(2,3,5,-1))
print(A)

#     [,1] [,2] [,3] [,4]
#[1,]    2    0    0    0
#[2,]    0    3    0    0
#[3,]    0    0    5    0
#[4,]    0    0    0   -1

result_9<-solve(A) %*% A - diag(x=4)
print(result_9)

#     [,1] [,2] [,3] [,4]
#[1,]    0    0    0    0
#[2,]    0    0    0    0
#[3,]    0    0    0    0
#[4,]    0    0    0    0

